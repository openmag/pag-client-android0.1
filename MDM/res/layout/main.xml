<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <Button android:id="@+id/admin" android:text="@string/enable" android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/function" android:textAppearance="?android:attr/textAppearanceLarge" android:text="@string/function"></TextView>
    <TextView android:text="@string/list" android:id="@+id/list" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp"></TextView>
</LinearLayout>
